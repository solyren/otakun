{
  "name": "otakun",
  "version": "0.2.0",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "start": "bun ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier . --write",
    "scan": "bun ./src/scripts/scan.ts",
    "equalize": "bun ./src/scripts/equalize.ts"
  },
  "dependencies": {
    "@upstash/redis": "^1.35.4",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^17.2.2",
    "elysia": "^1.4.9",
    "hono": "^4.9.8",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bun": "latest",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "glob-parent": "^6.0.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.44.0"
  }
}
